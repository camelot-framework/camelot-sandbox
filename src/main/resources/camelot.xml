<?xml version="1.0" encoding="UTF-8"?>
<plugins-config xmlns="urn:config.camelot.qatools.yandex.ru">
    <source>
        <artifact>${project.groupId}:${project.artifactId}:${project.version}</artifact>
        <broker>
            <param name="concurrentConsumers" value="10"/>
            <param name="maxConcurrentConsumers" value="50"/>
            <param name="destination.consumer.prefetchSize" value="1"/>
            <param name="maxMessagesPerTask" value="1"/>
        </broker>
        <plugin id="collector">
            <aggregator>sandbox.Collector</aggregator>
            <resource>sandbox.InputResource</resource>
        </plugin>
    </source>
</plugins-config>